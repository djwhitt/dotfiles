set cleaner ~/.config/lf/scripts/cleaner
set previewer ~/.config/lf/scripts/previewer

$mkdir -p ~/.trash

cmd trash $IFS="$(printf '\n\t')"; mv $fx ~/.trash

map D trash

map am $$HOME/.local/bin/archive-media "$f"

# Bookmark commands
cmd bookmark-set $~/.config/lf/scripts/bookmark-manager set
cmd bookmark-jump $~/.config/lf/scripts/bookmark-manager jump
cmd bookmark-delete $~/.config/lf/scripts/bookmark-manager delete

# Bookmark keybindings
map bs bookmark-set
map bb bookmark-jump
map bd bookmark-delete

# Claude Code
cmd claude-code $~/.local/bin/launch-claude-in-kitty-tab "$f"
map C claude-code
